<%= form_for @contact do |f| %>
	<p>
		<%= f.label :full_name %>
		<%= f.text_field :full_name %>
	</p>
	<p>
		<%= f.label :phone_number %>
		<%= f.text_field :phone_number %> 
	</p>
	<p>
		<%= f.label :email %>
		<%= f.text_field :email %>
	</p>
	<p>
		<%= f.label :address %>
		<%= f.text_area :address %>
	</p>
	<p>
		<%= f.submit class: "btn btn-success" %>
	</p>
<% end %>